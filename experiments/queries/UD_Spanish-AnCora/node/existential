MATCH (N1:Word)
MATCH (N2:Word)
MATCH (N3:Word)
MATCH (N1)-[:LEMMA]->(:Lemma {value: 'haber'})
MATCH (N1)-[:DEPREL {deprel: 'obj'}]->(N2)
MATCH (N2)-[:UPOS]->(:Upos {value: 'NOUN'})
MATCH (N2)-[:DEPREL {deprel: 'det'}]->(N3)
MATCH (N3)-[:UPOS]->(:Upos {value: 'DET'})
MATCH (N3)-[:DEFINITE]->(:Definite {value: 'Ind'})
RETURN N1, N2, N3